    "ex",
    "generator",
    "generate",
    "gen",
    "typescript",
    "ts",
    "types",
    "typing",
    "js",
    "new",
    "app",
    "express-ts",
    "decorators",
    "decorate",
    "test",
    "testing",
    "routes",
    "router",
    "routing"
  ],
  "main": "lib/cli.js",
  "bin": {
    "express-react-generator-typescript": "bin/cli.js",
    "ergt": "bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neotan/express-react-generator-typescript.git"
  },
  "author": "Neo Tan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neotan/express-react-generator-typescript/issues"
  },
  "homepage": "https://github.com/neotan/express-react-generator-typescript#readme",
  "scripts": {
    "express-react-generator-typescript": "node bin/cli.js",
    "start": "node bin/cli.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "edit-json-file": "^1.3.2",
    "ncp": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run --prefix ./templates/express-jest-eslint-es6 lint && npm run --prefix ./templates/express-jest-eslint-ts lint"
    }
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "npm-check-updates": "^4.0.4",
    "prettier": "^1.19.1"
  }
